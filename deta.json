{
    "name": "H2O-Short-Url",
    "description": "一款部署在多种平台,基于Python-Flask框架,开发的短网址程序.",
    "env": [
        {
            "key": "DATABASE_HOST",
            "description": "数据库地址",
            "required": true 
        },
        {
            "key": "DATABASE_PORT",
            "description": "数据库端口",
            "required": true 
        },
        {
            "key": "DATABASE_USERNAME",
            "description": "数据库用户名",
            "required": true 
        },
        {
            "key": "DATABASE_PASSWORD",
            "description": "数据库密码",
            "required": true 
        },
        {
            "key": "DATABASE_NAME",
            "description": "数据库名称",
            "required": true 
        },
        {
            "key": "DATABASE_PREFIX",
            "description": "数据库前缀,推荐使用h2o_short_url_",
            "required": true 
        }
    ]      
}